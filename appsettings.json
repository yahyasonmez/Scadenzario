{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": ""
  },
  "Smtp": {
    "Port": 25,
    "Security": "None",
    "Sender": "Scadenzario <noreply@example.org>"
  },
  "MemoryCache": {
    "SizeLimit": 1000
  },
  "ResponseCache": {
    "Home": {
      "Duration": 60,
      "Location": "Client",
      "VaryByQueryKeys": [ "page" ]
    }
  },
  "Scadenze": {
    "PerPage": 10,
    "Order": {
      "By": "DataScadenza",
      "Ascending": false,
      "Allow": [ "DataScadenza", "Beneficiario", "Importo" ]
    }
  },
  "Beneficiari": {
    "PerPage": 10,
    "Order": {
      "By": "Descrizione",
      "Ascending": false,
      "Allow": [ "Beneficiario", "Descrizione" ]
    }
  },
  "ReCaptcha": {
    "SecretKey": "https://www.google.com/recaptcha/admin",
    "SiteKey": "https://www.google.com/recaptcha/admin",
    "Version": ""
  },
  "LoginFacebook": {
    "AppId": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio",
    "AppSecret": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio"
  },
  "LoginGoogle": {
    "ClientID": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio",
    "ClientSecret": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio"
  },
  "LoginTwitter": {
    "consumerkey": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio",
    "consumersecret": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio"
  },
  "LoginMicrosoft": {
    "id": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio",
    "key": "https://docs.microsoft.com/it-it/aspnet/core/security/authentication/social/?view=aspnetcore-5.0&tabs=visual-studio"
  },
  "LoginYahoo": {
